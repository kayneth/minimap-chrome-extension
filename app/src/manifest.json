{
  "name": "Minimap extension",
  "version": "1.0",
  "description": "Extension to add a IDE-like minimap",
  "author": "Dylan Ballandras",
  "permissions": ["declarativeContent", "activeTab"],
  "content_scripts": [
    {
      "js": ["main.iife.min.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "logo.png"
  },
  "manifest_version": 2
}